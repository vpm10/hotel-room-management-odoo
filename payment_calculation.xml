<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="payment_calculation_view" model="ir.ui.view">
            <field name="name">payment.calculation.tree</field>
            <field name="model">payment.calculation</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                    <field name="quantity"/>
                    <field name="uom_id"/>
                    <field name="currency_id" invisible="1"/>
                    <field  name="unit_price"/>
                    <field name="sub_total"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>